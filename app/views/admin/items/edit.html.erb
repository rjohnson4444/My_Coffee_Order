<h1>Edit Items</h1>

<%= form_for(@item, url: admin_item_path) do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.label :price %>
<%= f.text_field :price %>
<%= f.label :image %>
<%= f.text_field :image %>
<%= f.submit %>
<% end %>
